<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.route"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.route.Route"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.route.List"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name="com.example.route.FinalRoute"
            android:label="@string/title_activity_final_route" >
        </activity>
        <activity
            android:name="com.example.route.List2"
            android:label="@string/title_activity_list2" >
        </activity>
    </application>

</manifest>