<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.actionbardemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.actionbardemo.ActionBarDemo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.actionbardemo.Call"
            android:label="@string/title_activity_call" >
        </activity>
        <activity
            android:name="com.example.actionbardemo.Contact"
            android:label="@string/title_activity_contact" >
        </activity>
        <activity
            android:name="com.example.actionbardemo.Chat"
            android:label="@string/title_activity_chat" >
        </activity>
        <activity
            android:name="com.example.actionbardemo.EventDemo"
            android:label="@string/title_activity_event_demo" >
        </activity>
    </application>

</manifest>