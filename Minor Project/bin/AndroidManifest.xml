<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigationdrawer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.navigationdrawer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.navigationdrawer.Eventt"
            android:label="@string/title_activity_eventt" >
        </activity>
        <activity
            android:name="com.example.navigationdrawer.Staions"
            android:label="@string/title_activity_staions" >
        </activity>
        <activity
            android:name="com.example.navigationdrawer.FragmentFour"
            android:label="@string/title_activity_fragment_four" >
        </activity>
        <activity
            android:name="com.example.navigationdrawer.FragmentFive"
            android:label="@string/title_activity_fragment_five" >
        </activity>
        <activity
            android:name="com.example.navigationdrawer.ProgressBar"
            android:label="@string/title_activity_progress_bar" >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>